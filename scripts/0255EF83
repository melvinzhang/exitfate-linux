  #--------------------------------------------------------------------------
  # * Read war setup data and create player-side units
  #--------------------------------------------------------------------------              
  def create_player_units(units, side, locs)
    for i in 0...locs.size
      # Leader stats
      leader = $data_warleaders[i+1]
      if not $game_actors[leader.required_char].selectable
        next
      end
      portraits = [leader.name]
      men = leader.men
      attack = leader.attack
      defence = leader.defence
      abilities = []        
      type = $game_party.war_units[i+1][0]
      # Penalties/Bonuses
      if type == UNIT_INFANTRY
        attack += 1
        defence += 1
      elsif type == UNIT_SORCERY
        attack -= -1
      end        
      if leader.rating[type] == "B"
        attack -= 1
        defence -= 1
      elsif leader.rating[type] == "C"
        attack -= 2
        defence -= 2
      end        
      # Member stats
      for id in $game_party.war_units[i+1][1]
        if id > 0
          member = $data_warmembers[id]
          portraits.push(member.name)
          men += member.men
          attack += member.attack
          defence += member.defence
          if member.ability > 0
            abilities.push(member.ability)
          end
        end
      end        
      attack = [attack, 0].max
      defence = [defence, 0].max
      # Create unit
      x = locs[i][0]
      y = locs[i][1]
      units.push(Game_WarUnit.new(x, y, leader.name, side, type,
              men, attack, defence, abilities, portraits, (i == 0), true, false, 0, leader.deathmessage))     
    end
  end
