#==============================================================================
# ** Data_WarBattles
#------------------------------------------------------------------------------
#  Sets up war battle units
#
#  Put units in order of movement (Sorcery -> Scout -> Infantry -> Cavalry -> Leader unit)
#==============================================================================
          
class Data_WarBattles
  #--------------------------------------------------------------------------
  # * Set up
  #
  #  Returns [units, turn limit for A score, 
  #--------------------------------------------------------------------------
  def setup(id)    
    if (id < 100 and id > 16) or (id >= 100 and id > 109)
      return nil
    end
    return eval("setup_battle_" + id.to_s)
  end
  #--------------------------------------------------------------------------
  # * Battle 1: Jargo Port City
  #--------------------------------------------------------------------------
  def setup_battle_1
    units = []
    # Kirgard Side
    units.push(Game_WarUnit.new(5, 11, "Daniel", SIDE_KIRGARD, UNIT_INFANTRY,
                400, 6, 6, [], ["Daniel"], false, true, false, 0, "Is this... the end?"))       
    units.push(Game_WarUnit.new(12, 6, "Angel", SIDE_KIRGARD, UNIT_INFANTRY,
                525, 5, 6, [WA_BLOCKADE],  ["Angel"], false, true, true, 1, "What a foolish mistake..."))  
