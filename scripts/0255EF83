    units.push(Game_WarUnit.new(4, 11, "Kirgard 3rd Legion", SIDE_KIRGARD, UNIT_INFANTRY,
                350, 3, 4, [], [], false, true, false))   
    units.push(Game_WarUnit.new(4, 12, "Kirgard 3rd Legion", SIDE_KIRGARD, UNIT_SCOUT,
                325, 3, 3, [WA_COUNTER], [], false, true, false))                             
    # Jargo Side
    units.push(Game_WarUnit.new(13, 15, "Jargo Defence Force", SIDE_ZELMONY, UNIT_INFANTRY,
                450, 4, 4, [], [], false, false, true))              
    units.push(Game_WarUnit.new(12, 15, "Jargo Defence Force", SIDE_ZELMONY, UNIT_INFANTRY,
                400, 3, 5, [], [], false, false, true))             
